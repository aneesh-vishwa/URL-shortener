{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A simple URL shortener built with Node.js, Express, MongoDB, and React.",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "start": "node server.js"
  },
  "keywords": [
    "url",
    "shortener",
    "node",
    "express",
    "react",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "mongoose": "^8.0.0",
    "shortid": "^2.2.16",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  }
}
  